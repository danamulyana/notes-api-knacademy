{
    "version": 2,
    "builds": [
        {
            "src": "docs/**",
            "use": "@vercel/static"
        },
        { "src": "app.js", "use": "@vercel/node" }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "app.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            },
            "maxDuration": 60
        },
        {
            "src": "/",
            "dest": "docs/index.html"
        }
    ]
}