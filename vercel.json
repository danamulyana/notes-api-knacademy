{
    "version": 2,
    "builds": [
        {
            "src": "docs/**",
            "use": "@vercel/static"
        },
        { "src": "app.js", "use": "@vercel/node" }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "app.js"
        },
        {
            "src": "/",
            "dest": "docs/index.html"
        }
    ]
  }
  